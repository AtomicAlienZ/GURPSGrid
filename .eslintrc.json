{
  "env": {
    "browser": true,
    "node": true,
    "shared-node-browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "allowImportExportEverywhere": true,
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "airbnb",
    "plugin:react/recommended"
  ],
  "settings": {},
  "globals": {},

  "rules": {
    "comma-dangle": [ "warn", "always-multiline" ],
    "no-undef": "warn",
    "quotes": ["warn", "single"],
    "react/jsx-tag-spacing": ["warn"],
    "space-before-function-paren": 0,
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "no-param-reassign": ["warn", { "props": false }],
    "react/prop-types": ["warn"],
    "react/require-default-props": 0,
    "func-names": ["warn", "as-needed"],
    "no-multiple-empty-lines": ["warn", {"max": 1}],
    "no-bitwise": ["warn", {"int32Hint": true}],
    "react/forbid-prop-types": ["warn", { "forbid": ["any"] }],
    "import/first": ["warn"],
    "consistent-return": ["warn"],
    "spaced-comment": 0,
    "react/jsx-no-bind": ["warn", {"allowArrowFunctions": false}],
    "padded-blocks": ["warn", { "blocks": "never", "classes": "never", "switches": "never" }],
    "react/jsx-filename-extension": ["warn"],
    "semi": "warn",
    "max-len": [
      "warn",
      {
        "code": 120,
        "ignoreComments": true,
        "ignoreUrls": true,
        "ignoreRegExpLiterals": true,
        "ignoreTemplateLiterals": true
      }
    ],
    "no-unused-vars": "warn",
    "react/no-unused-prop-types": ["warn", { "customValidators": ["isValid"], "skipShapeProps": true }],
    "no-use-before-define": "warn",
    "react/jsx-wrap-multilines": ["warn", { "declaration": true, "assignment": true, "return": true, "arrow": true }],
    "react/jsx-boolean-value": ["warn", "always"],
    "object-shorthand": ["warn"],
    "prefer-const": 0,
    "react/jsx-no-target-blank": ["warn"],
    "no-alert": ["error"],

    "array-bracket-spacing": 0,

    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,

    "react/jsx-indent-props": 0,
    "import/extensions": 0,
    "no-underscore-dangle": 0,
    "react/jsx-indent": 0,
    "comma-spacing": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/no-array-index-key": 0,
    "arrow-body-style": 0,
    "object-curly-spacing": 0,
    "react/jsx-first-prop-new-line": 0,
    "arrow-parens": 0,
    "eqeqeq": 0,
    "react/sort-comp": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/label-has-for": 0,
    "import/prefer-default-export": 0,
    "no-plusplus": 0,

    "brace-style": 0,
    "react/no-string-refs": 0,
    "indent": 0,
    "class-methods-use-this": 0,
    "no-extra-semi": 0,
    "one-var": 0,
    "prefer-template": 0,
    "space-in-parens": 0,
    "no-mixed-operators": 0,
    "react/prefer-stateless-function": 0,
    "no-shadow": 0,
    "block-spacing": 0,
    "quote-props": 0,
    "prefer-arrow-callback": 0,
    "camelcase": 0,
    "no-nested-ternary": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "space-before-blocks": 0,
    "no-prototype-builtins": 0,
    "no-else-return": 0,
    "react/no-unescaped-entities": 0,
    "react/no-find-dom-node": 0,
    "no-unreachable": 0,
    "radix": 0,
    "dot-notation": 0,
    "global-require": 0,
    "react/display-name": 0,
    "react/jsx-curly-spacing": 0,
    "import/no-duplicates": 0,
    "meteor/audit-argument-checks": 0,
    "key-spacing": 0,
    "arrow-spacing": 0,
    "jsx-quotes": 0,
    "react/self-closing-comp": 0,
    "no-return-assign": 0,
    "no-lone-blocks": 0,
    "space-infix-ops": 0,
    "no-duplicate-imports": 0,
    "default-case": 0,
    "jsx-a11y/href-no-hash": 0,
    "no-restricted-syntax": 0,
    "no-multi-spaces": 0,
    "no-confusing-arrow": 0
  }
}
